<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<class name="entity.house" table="HOUSE" lazy="false">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="sequence">
				<param name="sequence">seq1</param>
			</generator>
		</id>
		
		<many-to-one 
			lazy="false"
        	name="users" 
            column="USER_ID"
        	class="entity.users">
        </many-to-one>
        
        <many-to-one 
        	lazy="false"
        	name="types" 
            column="TYPE_ID"
        	class="entity.types">
        </many-to-one>
        
        <many-to-one 
        	lazy="false"
        	name="street" 
            column="STREET_ID"
        	class="entity.street">
        </many-to-one>
		
		
		<property name="title" type="java.lang.String" column="TITLE"></property>
		<property name="description" type="java.lang.String" column="DESCRIPTION"></property>
		<property name="price" type="java.lang.Double" column="PRICE"></property>
		<property name="pubdate" type="java.util.Date" column="PUBDATE"></property>
		<property name="floorrage" type="java.lang.Double" column="FLOORAGE"></property>
		<property name="contact" type="java.lang.String" column="CONTACT"></property>
	</class>
</hibernate-mapping>
